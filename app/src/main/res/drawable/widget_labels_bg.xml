<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
  <item>
    <shape android:shape="rectangle">
      <solid android:color="#a6a6a6" />
      <corners android:radius="1dp" android:topLeftRadius="5dp" android:topRightRadius="0dp"
               android:bottomLeftRadius="5dp" android:bottomRightRadius="0dp"/>
    </shape>
  </item>
  <item android:top="1dp" android:left="1dp" android:bottom="1dp">
    <shape android:shape="rectangle">
      <solid android:color="#fff" />
      <corners android:radius="1dp" android:topLeftRadius="5dp" android:topRightRadius="0dp"
               android:bottomLeftRadius="5dp" android:bottomRightRadius="0dp"/>
    </shape>
  </item>
  <item android:top="1dp" android:left="1dp" android:right="1dp" android:bottom="1dp">
    <shape android:shape="rectangle">
      <solid android:color="#d8d8d8" />
      <corners android:radius="1dp" android:topLeftRadius="5dp" android:topRightRadius="0dp"
               android:bottomLeftRadius="5dp" android:bottomRightRadius="0dp"/>
    </shape>
  </item>
  <item android:top="1dp" android:left="1dp" android:right="2dp" android:bottom="1dp">
    <shape android:shape="rectangle">
      <solid android:color="#f1f1f1" />
      <corners android:radius="1dp" android:topLeftRadius="5dp" android:topRightRadius="0dp"
               android:bottomLeftRadius="5dp" android:bottomRightRadius="0dp"/>
    </shape>
  </item>

</layer-list>
